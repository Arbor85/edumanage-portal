# API Configuration
VITE_API_BASE_URL=http://localhost:8000

# Auth0 Configuration
# Get these values from your Auth0 dashboard at https://manage.auth0.com

# Your Auth0 domain (e.g., your-app.us.auth0.com)
VITE_AUTH0_DOMAIN=your_auth0_domain_here

# Your Auth0 application Client ID
VITE_AUTH0_CLIENT_ID=your_client_id_here

# (Optional) Auth0 API Audience for access tokens
# VITE_AUTH0_AUDIENCE=https://your-api.example.com

# OpenAI API Configuration
# Get your API key from https://platform.openai.com/api-keys
VITE_OPENAI_API_KEY=your_openai_api_key_here
